ALGORITHM DuplicateElements
VAR
 arr1 = [12, 13, 6, 10];
 arr2 = [13, 10, 16, 15];
 i,j,sum= 0;
BEGIN

FOR i FROM 0 TO arr1.length-1 STEP 1  DO
     j = 0;
     WHILE (j<arr2.length-1) DO
          if (arr1[i] == arr2[j]){   
            sum += arr1[i] + arr2[j];
        }
        j++;
     END_WHILE
   
END_FOR
  
 print("Sum of duplicated Element",sum)
END